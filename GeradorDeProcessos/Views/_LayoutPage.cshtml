								<a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Administração <span class="caret"></span></a>
								<ul class="dropdown-menu" role="menu">
									<li><a href="@Url.Action("Index", "Empresas")">Gerenciar Empresas</a></li>
									<li><a href="@Url.Action("Index", "Empreendimentos")">Gerenciar Empreendimentos</a></li>
									<li><a href="@Url.Action("Index", "Usuarios")">Gerenciar Usuários</a></li>
									<li class="divider"></li>
									<li><a href="@Url.Action("Logout", "Home")">Sair</a></li>
								</ul>
							</li>
							<li><a href="@Url.Action("Logout", "Home")">Sair</a></li>
						</ul>
					</div>
				</div>
			</nav>
			@*--------------------fim da barra superior --------------------*@
			@RenderBody()
		</div>

        <div class="row">
            <div class="form-top-custom-black" style="text-align:center">
                <address>
                    <p>
                        <span class="glyphicon glyphicon-phone-alt"></span>
                        Telefone: (11) 5081-7969
                        <br />
                        <span class="glyphicon glyphicon-envelope"></span>
                        Email: analistas@isibr.com.br
                    </p>
                </address>
            </div>
        </div>


	</div>
	<script src="@Url.Content("~/Scripts/jquery-3.1.0.min.js")"></script>
	@*<script src="@Url.Content("~/Scripts/jquery-1.9.1.min.js")"></script>*@
	<script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
	<script src="@Url.Content("~/Scripts/jquery.maskedinput.min.js")"></script>
	<script src="@Url.Content("~/Scripts/jquery.maskMoney.min.js")"></script>
	@RenderSection("scripts", required: false)
