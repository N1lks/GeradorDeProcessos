    <link href="@Url.Content("~/Content/PagedList.css")" rel="stylesheet" />
    

	<title>Procex - @ViewBag.Title</title>
</head>
<body>
	

	<div class="container" style="margin-top:60px">
		<div class="panel panel-collapse" style="padding:10px">
			@*<div class="panel panel-collapse" id="perfil">
				@{Html.RenderPartial("_Usuario"); }
			</div>*@
			@*-------------- Barra superior -------------------------*@
			<nav class="navbar navbar-default">
