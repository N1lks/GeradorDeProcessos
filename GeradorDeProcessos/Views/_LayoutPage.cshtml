
@{
	Layout = null;
}

<!DOCTYPE html>

<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width" />
	<link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" />
	<title>@ViewBag.Title</title>
</head>
<body>
	<nav class="navbar navbar-default navbar-fixed-top">
		<div class="container">
			<div class="navbar-header">
				<a class="navbar-brand" href="#"><img height="25" src="~/Content/logo.jpg" /></a>
			</div>
			<div class="collapse navbar-collapse">
				<ul class="nav navbar-nav">
					<li id="menuHome"><a href="@Url.Action("Index", "Home")">Empreendimentos</a></li>
					<li class="dropdown" id="menuAdministracao">
						<a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Administração <span class="caret"></span></a>
						<ul class="dropdown-menu" role="menu">
							<li><a href="@Url.Action("Index", "Empreendimentos")">Gerenciar Empreendimentos</a></li>
							<li><a href="#">Gerenciar Usuários</a></li>
							<li><a href="#">Suporte</a></li>
							<li class="divider"></li>
							<li><a href="#">Separated link</a></li>
							<li class="divider"></li>
							<li><a href="#">One more separated link</a></li>
						</ul>
					</li>
				</ul>
			</div>
		</div>
	</nav>
	<div class="container" style="margin-top:40px">
		@RenderBody()
	</div>
	<script src="http://code.jquery.com/jquery-1.9.1.js"></script>
	@*<script src="@Url.Content("~/Scripts/jquery-3.1.0.min.js")"></script>*@
	<script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>

	@RenderSection("scripts", required: false)
</body>
</html>
