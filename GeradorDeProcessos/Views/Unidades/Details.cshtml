		</dt>

		<dd>
			@Html.DisplayFor(model => model.UnidadeStatus)
		</dd>
		<dt>
			@Html.LabelFor(model => model.Tipo)
		</dt>

		<dd>
			@Html.DisplayFor(model => model.Tipo)
		</dd>
		<dt>
			@Html.LabelFor(model => model.UnidadeObservacao)
		</dt>

		<dd>
			@Html.DisplayFor(model => model.UnidadeObservacao)
		</dd>

